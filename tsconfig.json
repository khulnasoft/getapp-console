{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "lib": ["webworker", "DOM", "ESNext"],
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "Node",
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}
